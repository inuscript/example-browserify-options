{
  "name": "example-browserify-options",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "default": "browserify -e src/entry1.js > build/default.js",
    "external": "browserify -e src/entry1.js -x lib/*.js > build/external.js",
    "exclude": "browserify -e src/entry1.js -u lib/*.js > build/exclude.js",
    "require": "browserify -r ./lib/*.js > build/require.js",
    "start": "npm run default; npm run external; npm run exclude ; npm run require;"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^11.0.1"
  }
}
